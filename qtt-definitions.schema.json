                        "transformations": [{
                            "type": "prefix",
                            "params": [
                                "EFSA_SSD2:"
                            ]
                        }]
                    }]
                ]
            },
            "drop_rows": {
                "type": "array",
                "title": "Defines rows to drop. Indexed as in Excel.",
                "items": {
                    "type": "integer",
                    "examples": [
                        33,
                        51,
                        105,
                        154
                    ]
                },
                "examples": [
                    [33,
                        51],
                    [105,
                        154
